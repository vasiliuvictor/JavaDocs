<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">
    
    <changeSet id="victor-1.0" author="The Victor">
        <createTable tableName="VictorPeoples">
            <column name="PERS_ID" type="number(6)">
                <constraints primaryKey="true"></constraints>
            </column>
            <column name="FIRST_NAME" type="varchar2(40 BYTE)"></column>
            <column name="LAST_NAME" type="varchar2(40 BYTE)"></column>
            <column name="EMAIL" type="varchar2(30 BYTE)">
                <constraints nullable="false"></constraints>
            </column>
            <column name="PHONE_NUMBER" type="varchar2(25 BYTE)"></column>
            <column name="ADRESS_ID" type="number(6)">
                <constraints nullable="false"></constraints>
            </column>

        </createTable>
    </changeSet>

</databaseChangeLog>
